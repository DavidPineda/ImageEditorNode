{
	"version": "0.2.0",
	"configurations": 
	[
		{
			"name": "Launch Linux",
			"type": "node",
			"request": "launch",
			"program": "server.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": ".",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "?~~~~"
		},
		{
			"name": "Launch Windows",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/server.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}/",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/"
		},		
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}	
	]
}